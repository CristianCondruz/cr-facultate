
{% trans_default_domain 'FOSUserBundle' %}

<h1>Register!</h1>

{{ form_start(form, {'method': 'post', 'action': path('student_registration'), 'attr': {'class': 'fos_user_registration_register'}}) }}
  
  {#{{ form_row(form.nume) }}#}
  {#{{ form_row(form.prenume) }}#}
  {{ form_row(form.email) }}
  {{ form_row(form.plainPassword) }}
  {#{{ form_row(form.roles) }}#}

  <div>
    <input class="btn btn-primary" type="submit" value="{{ 'registration.submit'|trans }}" />
  </div>
{{ form_end(form) }}